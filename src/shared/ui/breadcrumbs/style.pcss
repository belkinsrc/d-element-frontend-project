.breadcrumbs {
    margin-top: 24px;
}

.breadcrumbs__list {
    list-style: none;
    display: flex;
}

.breadcrumbs__item {
    color: #888888;
    font-size: 14px;
    font-weight: 400;
    line-height: 24px;
    cursor: default;
}

.breadcrumbs__link {
    text-decoration: none;
    color: #888888;
}

.breadcrumbs__separator {
    display: inline-block;
    margin: 0 6px 0 9px;
}